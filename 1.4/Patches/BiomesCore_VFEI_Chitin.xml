<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Insectoids</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="VFEI_Chitin"]/stuffProps/categories</xpath>
					<value>
						<li>BMT_ChitinStuff</li>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName="VFEI_Chitin"]/label</xpath>
					<value>
						<label>insectoid chitin</label>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>